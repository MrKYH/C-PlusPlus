#include <iostream>

using namespace std;

int main()
{
    int q = 1;
    int w = 2;
    int e = 3;
    int r = 4;
    int row[4] = {q,w,e,r};
    int highestRow = 0;

    for(int i=0;i<4;i++)
    {
        if(row[i]>highestRow)
        highestRow=row[i];
    }
    cout << "The biggest number is: " << highestRow << endl;
}
